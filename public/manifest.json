{
  "manifest_version": 2,

  "name": "ekitan-trainno",
  "description": "Add the display of the train number to the train timetable.",
  "version": "0.0.1",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["https://ekitan.com/timetable/railway/train*"],
      "js": ["js/vendor.js", "js/content_script.js"]
    }
  ]
}
